{
  "name": "qbaseclient",
  "version": "2.0.0",
  "description": "Client for QBaseServer",
  "scripts": {
    "build": "yarn clean && nuxt build",
    "check:i18n": "yarn run i18n --check",
    "clean": "rimraf .nuxt dist",
    "dev": "nuxt",
    "gen:dotenv": "node scripts/gen-dotenv.js",
    "i18n": "sync-i18n --files 'locales/*.json' --primary en --languages en ru --space 2 --finalnewline",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --max-warnings 0 .",
    "start": "nuxt start"
  },
  "engines": {
    "node": "16.x"
  },
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@nuxt/typescript": "^2.8.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^12.0.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "angular-html-parser": "^1.2.0",
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "i18next-json-sync": "^2.3.1",
    "lodash.isequal": "^4.5.0",
    "material-design-icons-iconfont": "^5.0.1",
    "nuxt": "^2.8.0",
    "nuxt-property-decorator": "^2.1.3",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "stylus-loader": "^3.0.2",
    "stylus": "^0.54.5",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1",
    "vue-i18n": "^8.11.2",
    "vue": "^2.6.10",
    "vue-katex": "^0.2.0",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^1.5.14",
